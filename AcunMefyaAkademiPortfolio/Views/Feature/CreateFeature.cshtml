﻿@model AcunMefyaAkademiPortfolio.Models.TblFeature
@{
    ViewBag.Title = "CreateFeature";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>CreateFeature</h2>

<form method="post">
    <label>AdSoyad</label>
    <textarea rows="4" class="table table-bordered" name="NameSurname"></textarea>
    <br />
    <label>Proje Açıklaması</label>
    <textarea rows="4" class="table table-bordered" name="Description"></textarea>
    <br />
    <label> Proje Adı</label>
    <input type="text" class="table table-bordered" name="ProjectName" />
    <br />
    <label>Resim Url</label>
    <input type="text" class="table table-bordered" name="ImageUrl" />
    <br /> 
    <button class="btn btn-info">Projeyi Kaydet</button>
</form>
